{% extends uns ~ '/front/layout.html.twig' %}

{% block user_login %}

{{ form(user_login_form) }}

{% if _register_route %}
<p>
<a href="{{ path(_register_route) }}">{{ (duns ~ 'login.labels.register')|trans }}</a>
</p>
{% endif %}

{% if _recover_password_route %}
<p>
<a href="{{ path(_recover_password_route) }}">{{ (duns ~ 'login.labels.recover_password')|trans }}</a>
</p>
{% endif %}


{% endblock user_login %}